generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URL")
}

model user {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  email       String   @unique
  firstName   String?
  lastName    String?
  imageUrl    String?
  clerkUserId String   @unique
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
